configuration {
modes: "combi";
font: "FontAwesome 18";
      show-icons: true;
      disable-history: false;
      combi-modes: "window,drun,run";
      display-drun: "" ;
      display-combi:"" ;
      timeout {
action: "kb-cancel";
delay:  0;
      }
      filebrowser {
          directories-first: true;
          sorting-method:    "name";
      }
}

@theme "mytheme"
